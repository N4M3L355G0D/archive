<qemu>
	<version num="1">
		<CD>/srv/samba/middle/Torrents/ubuntu-17.10-desktop-amd64.iso</CD>
		<IMG>ubu.img</IMG>
		<IMG_Size>10G</IMG_Size>
		<CMD>qemu-system-x86_64</CMD>
		<cpu>host</cpu>
		<accel>kvm</accel>
		<ram>4G</ram>
		<cores>4</cores>
		<vga>std</vga>
		<display>gtk</display>
		<DB>qemuVM.db</DB>
		<name>ubu.img</name>
		<nicModel>pcnet</nicModel>
		<soundHW>hda</soundHW>
	</version>
	<!-- to add another configuration use the above version node as a template
       and increment num by 1 -->

</qemu>
